<div class="discourse-top-contributors side-nav">
  <h1 class="side-nav-heading">
    {{d-icon "user"}}
    Top Contributors
  </h1>

  <div class="top-contributors-container side-nav-container-body">
    {{#each topContributors as |user|}}
      {{top-contributor data=user}}
    {{/each}}
    <div class="top-contributors-container-link">
      <a href="/u?order=likes_received&period=yearly">
        {{theme-i18n "top_contributors.view_all"}}
      </a>
    </div>
  </div>
</div>